{
  "version": 1,
  "routes": [
    {
      "src": "/movie-details",
      "dest": "/movie_details/movie_details.html"
    },
    {
      "src": "/anime-test",
      "dest": "/anime_test.html"
    },
    {
      "src": "/anime-direct-player",
      "dest": "/anime_direct_player.html"
    },
    {
      "src": "/hls-test",
      "dest": "/hls_test.html"
    },
    {
      "src": "/proxy",
      "has": [
        {
          "type": "query",
          "key": "url"
        }
      ],
      "dest": "/worker-proxy"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
